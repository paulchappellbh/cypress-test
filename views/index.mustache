<html>
 <head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="#" data-test="page-title">{{ title }}</a>
    </nav>
    <div class="container p-3">
      {{#blocks}}
        {{#isHeroBlock}}
          <div class="jumbotron" data-test="{{ dataTest }}">
            <h1 class="display-4">{{ headingText }}</h1>
            <p class="lead">{{ paragraphText }}</p>
            <hr class="my-4">
            <a class="btn btn-primary btn-lg" href="{{ buttonUrl }}" role="button">{{ buttonText }}</a>
          </div>
        {{/isHeroBlock}}
        {{#isLogosBlock}}
          <div class="row my-5" data-test="{{ dataTest }}">
            {{#logos}}
              <div class="col-sm">
                <div class="card">
                  <img src="{{ imgUrl }}" class="card-img-top" alt="{{ altText }}" data-test="logos-block__image">
                  <div class="card-body">
                    <h5 class="card-title" data-test="logos-block__display-text">{{ displayText }}</h5>
                  </div>
                </div>
              </div>
            {{/logos}}
          </div>
        {{/isLogosBlock}}
        {{#isThreeColumnBlock}}
            <div class="row my-5" data-test="{{ dataTest }}">
              {{#columns}}
                <div class="col-sm-4">
                  <div class="card">
                    <div class="card-body">
                      <h5 class="card-title">{{ heading }}</h5>
                      <a href="{{ linkUrl }}">{{ linkText }}</a>
                    </div>
                  </div>
                </div>
              {{/columns}}
            </div>
        {{/isThreeColumnBlock}}
      {{/blocks}}
    </div>
 </body>
</html>